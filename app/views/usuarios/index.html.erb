<h1>Listing usuarios</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Apellido</th>
    <th>Fecnac</th>
    <th>Direccion</th>
    <th>Telefono</th>
    <th>Email</th>
    <th>Grupo</th>
    <th>Jefe</th>
    <th>Subjefe</th>
    <th>Observaciones</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @usuarios.each do |usuario| %>
  <tr>
    <td><%= usuario.name %></td>
    <td><%= usuario.apellido %></td>
    <td><%= usuario.fecnac %></td>
    <td><%= usuario.direccion %></td>
    <td><%= usuario.telefono %></td>
    <td><%= usuario.email %></td>
    <td><%= usuario.grupo %></td>
    <td><%= usuario.jefe %></td>
    <td><%= usuario.subjefe %></td>
    <td><%= usuario.observaciones %></td>
    <td><%= link_to 'Show', usuario %></td>
    <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
    <td><%= link_to 'Destroy', usuario, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Usuario', new_usuario_path %>
